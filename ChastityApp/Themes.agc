function ChangeTheme(themeSelected)
	// LIGHT
	rgbColor# = OryUIConvertColor("#F4F4F9") : colorMode[1].backgroundColor = MakeColor(rgbColor#[1], rgbColor#[2], rgbColor#[3])
	rgbColor# = OryUIConvertColor("#DEDEDE") : colorMode[1].barColor = MakeColor(rgbColor#[1], rgbColor#[2], rgbColor#[3])
