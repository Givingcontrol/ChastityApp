	colorMode[1].textfieldStrokeColor = theme[themeSelected].color[3]
	rgbColor# = OryUIConvertColor("#FFFFFF") : colorMode[1].menuColor = MakeColor(rgbColor#[1], rgbColor#[2], rgbColor#[3])
	colorMode[1].navigationDrawerColor = theme[themeSelected].color[3]
	colorMode[1].navigationDrawerOptionActiveOverlayColor = theme[themeSelected].color[3]
	colorMode[1].navigationDrawerStatusBarColor = theme[themeSelected].color[5]
	colorMode[1].navigationDrawerSubtitleColor = theme[themeSelected].color[5]
